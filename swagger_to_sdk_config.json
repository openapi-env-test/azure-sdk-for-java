{
  "$schema": "https://openapistorageprod.blob.core.windows.net/sdkautomation/prod/schemas/swagger_to_sdk_config.schema.json",
  "meta": {
    "autorest_options": {
      "version": "V2",
      "java": "",
      "verbose": "",
      "sdkrel:azure-libraries-for-java-folder": ".",
      "multiapi": "",
      "use": "@microsoft.azure/autorest.java@preview"
    },
    "version": "0.2.0",
    "advanced_options": {
      "create_sdk_pull_requests": true,
      "sdk_generation_pull_request_base": "integration_branch"
    }
  },
  "initOptions": {
    "initScript": {
      "path": "./eng/mgmt/automation/init.sh",
      "envs": [
        "JAVA_HOME_11_X64"
      ]
    }
  },
  "generateOptions": {
    "generateScript": {
      "path": "./eng/mgmt/automation/generate.py"
    }
  },
  "artifactOptions": {
    "installInstructionScript": {
      "path": "./eng/mgmt/automation/install_instruction.py"
    }
  },
  "advanceOptions": {
    "generationCallMode": "one-per-config"
  }
}
